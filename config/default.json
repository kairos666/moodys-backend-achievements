{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "secret": "-",
    "strategies": [ "apiKey" ],
    "apiKey": {
      "header": "x-api-key",
      "allowedKeys": [ "opensesame" ]
    }
  },
  "firebase": {
    "firebaseAppConfig": {
      "apiKey": "<api key>",
      "authDomain": "<app domain>",
      "databaseURL": "<app DB URL>",
      "storageBucket": "<app storage bucket>"
    },
    "firebaseAchievementsServiceAuth": {
      "email": "<dedicated user email>",
      "password": "<dedicated user password>"
    }
  }
}
